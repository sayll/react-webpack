{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    [
      "lodash",
      {
        "id": [
          "lodash"
          // "recompose"
        ]
      }
    ],
    [
      "react-css-modules",
      {
        "generateScopedName": "[name]_[local]_[hash:base64:5]"
      }
    ]
  ],
  "comments": false,
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
